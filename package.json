{
  "name": "sonicpi-samples",
  "version": "0.1.0",
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  },
  "private": true,
  "homepage": "https://sonicpisamples.100yen.co.uk",
  "dependencies": {
    "classnames": "^2.2.5",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.1.0",
    "iotacss": "^1.5.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "node-sass-chokidar": "0.0.3",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.1.1",
    "react-ga": "^2.3.5",
    "react-scripts": "^1.0.17"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "web-audio-test-api": "^0.5.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "build": "npm install --force && node-sass-chokidar src/ -o src/ && react-scripts build",
    "deploy": "gh-pages -d build",
    "dev": "npm-run-all -p watch-css start",
    "eject": "react-scripts eject",
    "lint": "eslint src/. --ext .js; exit 0;",
    "predeploy": "rm -rf node_modules/gh-pages/.cache && yarn run build",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom --watch --coverage --collectCoverageFrom=src/**/*js --collectCoverageFrom=!src/*.js",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  }
}
